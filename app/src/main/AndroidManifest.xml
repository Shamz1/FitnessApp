<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FitnessApp"
        tools:targetApi="31">
        <activity
            android:name=".Activty.ChatRoomActivity"
            android:exported="false" />
        <activity
            android:name=".Activty.Splash"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activty.Registration"
            android:exported="false" />
        <activity
            android:name=".Activty.Login"
            android:exported="false" />
        <activity
            android:name=".Activty.Programms"
            android:exported="false" />
        <activity
            android:name=".Activty.ClassesActivity"
            android:exported="false" />
        <activity
            android:name=".Activty.CalendarActivity"
            android:exported="false" />
        <activity
            android:name=".Activty.MainActivity"
            android:exported="false" />
    </application>

</manifest>